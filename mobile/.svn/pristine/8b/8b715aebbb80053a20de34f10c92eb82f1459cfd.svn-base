package com.jshuabo.mobile.server.model.batchinstall;

import com.jshuabo.frame.server.model.base.REntity;

/**
 * @ClassName: AppPackageInfoDetail
 * @Description:
 * @author: peng.wu
 * @date: Apr 1, 2014 4:13:12 PM
 */
public class AppPackageInfoDetail extends REntity {
    private static final long serialVersionUID = 5635103238537658888L;

    /**
     * @fieldName: lineNo
     * @fieldType: Integer
     * @Description: 行号
     */
    private Long lineNo;

    /**
     * @fieldName: packageInfo
     * @fieldType: AppPackageInfo
     * @Description: 应用包
     */
    private AppPackageInfo appPackageInfo = new AppPackageInfo();

    /**
     * @fieldName: appInfo
     * @fieldType: AppInfo
     * @Description: 应用信息
     */
    private AppInfo appInfo = new AppInfo();

    /**
     * @fieldName: description
     * @fieldType: String
     * @Description: 应用描述
     */
    private String description;


    public AppInfo getAppInfo() {
        return appInfo;
    }

    public void setAppInfo(AppInfo appInfo) {
        this.appInfo = appInfo;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public AppPackageInfo getAppPackageInfo() {
        return appPackageInfo;
    }

    public void setAppPackageInfo(AppPackageInfo appPackageInfo) {
        this.appPackageInfo = appPackageInfo;
    }

    public void setLineNo(Long lineNo) {
        this.lineNo = lineNo;
    }

    public Long getLineNo() {
        return lineNo;
    }


}
