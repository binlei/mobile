package com.jshuabo.mobile.server.web.controller.chart;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

/**
 * @ClassName: BaseChannelController
 * @Description: 
 * @author: jing.huang
 * @date: 2014年4月17日 下午2:08:08
 */
@Controller
@RequestMapping(value="/baseChannel")
public class BaseChannelController {
    @RequestMapping(value="/newArrival")
    public String newArrival() {
        return "channelStatics/newArrival/newArrivalMain";
    }
    @RequestMapping(value="/appActivated")
    public String appActivated() {
        return "channelStatics/appActivated/appActivatedMain";
    }
    @RequestMapping(value="/appUsage")
    public String appUsage() {
        return "channelStatics/appUsage/appUsageMain";
    }
    @RequestMapping(value="/popularApp")
    public String popularApp() {
        return "channelStatics/popularApp/popularAppMain";
    }
    
}
